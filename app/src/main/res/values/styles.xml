<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="RaisedButtonDark1" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">@color/color1</item>
        <item name="colorControlHighlight">@color/color1</item>
        <item name="android:textColor">@color/editTextColorWhite</item>
    </style>

    <style name="RaisedButtonDark" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">@color/colorPrimaryDark</item>
        <item name="colorControlHighlight">@color/colorPrimaryDark</item>
        <item name="android:textColor">@color/editTextColorWhite</item>
    </style>


</resources>
